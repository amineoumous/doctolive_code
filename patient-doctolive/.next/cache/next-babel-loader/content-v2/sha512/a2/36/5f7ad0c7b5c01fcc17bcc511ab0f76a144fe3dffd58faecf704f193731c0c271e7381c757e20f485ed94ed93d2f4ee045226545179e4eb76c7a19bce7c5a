witch-lang\",\n    children: /*#__PURE__*/_jsx(\"div\", {\n      className: \"\",\n      children: /*#__PURE__*/_jsxs(\"div\", {\n        className: \"row\",\n        children: [locale === \"en\" ? null : english(), locale === \"fr\" ? null : frensh(), locale === \"ar\" ? null : Arab()]\n      })\n    })\n  });\n}","map":null,"metadata":{},"sourceType":"module"}